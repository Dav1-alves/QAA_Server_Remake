<%- include ('../partials/header.ejs') %>

  <body>
    <%- include ('../partials/menuout.ejs') %>

      <section class="grid grid-cols-3 auto-rows-[298px]">
        <div class="bg-neutral-100 border-2 rounded-xl p-2 m-4">
          <h3 class="text-xl text-gray-600">1</h3>
          <p class="font-bold text-2xl">B</p>
        </div>
        <div class="bg-neutral-100 border-2 rounded-xl p-2 m-4">
          <h3 class="text-xl text-gray-600">2</h3>
          <p class="font-bold text-2xl">B</p>
        </div>
        <div class="bg-neutral-100 border-2 rounded-xl p-2 m-4 row-span-3">

          <% questions.forEach(question => { %>
            <div class="w-full p-3 rounded-xl bg-neutral-200 mb-2">
              <h3 class="text-xl text-gray-600 truncate"><%= question.name %></h3>
              <h4 class="font-bold py-2 text-gray-400"><% if (question.subCategory == null) { %>
                <%= question.Category.name %>
              <% } else { %>
                <%= question.subCategory.name %>
              <% } %></h4>
            </div>
          <% }) %>

        </div>
        <div class="bg-neutral-100 border-2 rounded-xl p-2 m-4 col-span-2">
          <h3 class="text-xl text-gray-600">4</h3>
          <p class="font-bold text-2xl">B</p>
        </div>
        <div class="bg-neutral-100 border-2 rounded-xl p-2 m-4">
          <h3 class="text-xl text-gray-600">5</h3>
          <p class="font-bold text-2xl">B</p>
        </div>
        <div class="bg-neutral-100 border-2 rounded-xl p-2 m-4 ">
          <h3 class="text-xl text-gray-600">6</h3>
          <p class="font-bold text-2xl">B</p>
        </div>
      </section>

  </body>
  <%- include ('../partials/footer.ejs') %>